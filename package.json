{
  "name": "ngraph.fromdot",
  "version": "7.0.0",
  "description": "Load dot files into ngraph.graph",
  "main": "index.js",
  "scripts": {
    "test": "tap --allow-incomplete-coverage test/*.js",
    "build": "browserify index.js -s fromDot -o dist/ngraph.fromDot.js && uglifyjs dist/ngraph.fromDot.js -o dist/ngraph.fromDot.min.js"
  },
  "keywords": [
    "ngraph",
    "dot",
    "graphviz",
    "graph",
    "serialization"
  ],
  "author": "Andrei Kashcha",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/anvaka/ngraph.fromdot"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "tap": "^19.2.0",
    "uglify-js": "^3.14.5"
  },
  "dependencies": {
    "dotparser": "^1.0.0",
    "ngraph.graph": "^20.0.0"
  }
}
